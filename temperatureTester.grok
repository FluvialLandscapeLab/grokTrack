	0, 1000, 2, 1, 2
	grade y
	0, 4, 2, 1, 2 
	end
	
generate layers interactive
	zone by layer
	
	base elevation
		elevation from xz pairs
		0.0 10.0
		1000.0 0.0
	end
	end
	
